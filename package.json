{
  "name": "my-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue serve --open DemoPage.vue",
    "test:unit": "vue-cli-service test:unit",
    "prebuild": "npm run eslint && npm run test:unit",
    "build": "vue-cli-service build --target lib --name vue-bitbucket-collapsible src/index.js",
    "eslint": "eslint 'src/**/*.vue' 'src/**/*.js'"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli": "^3.0.1",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.5.0",
    "vue-template-compiler": "^2.5.17"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/vue-bitbucket-collapsible.min.js",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
