{
  "name": "vue-translate3d-collapsible",
  "version": "1.0.0",
  "private": false,
  "description": "A simple collapsible using translate3d animations",
  "main": "dist/vue-translate3d-collapsible.umd.js",
  "module": "dist/vue-translate3d-collapsible.umd.min.js",
  "files": [
    "dist/*"
  ],
  "keywords": [
    "collapsible",
    "accessible",
    "accordion",
    "extendable"
  ],
  "scripts": {
    "serve": "vue serve --open DemoPage.vue",
    "test:unit": "vue-cli-service test:unit",
    "test:auto":"vue-cli-service test:unit --watch",
    "prebuild": "npm run eslint && npm run test:unit",
    "build": "vue-cli-service build --target lib --name vue-translate3d-collapsible src/index.js",
    "eslint": "eslint 'DemoPage.vue' 'src/**/*.vue' 'src/**/*.js'"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@vue/cli": "^3.0.1",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.5.0",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
